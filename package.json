{
  "name": "eslint-find-removed-rules",
  "version": "1.0.0",
  "description": "Find rules removed from a ESLint config",
  "keywords": [
    "eslint",
    "find",
    "removed",
    "rules"
  ],
  "homepage": "https://github.com/leepowelldev/eslint-find-removed-rules#readme",
  "bugs": {
    "url": "https://github.com/leepowelldev/eslint-find-removed-rules/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leepowelldev/eslint-find-removed-rules.git"
  },
  "license": "ISC",
  "author": "Lee Powell <lee@leepowell.dev>",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/cli.js",
    "lib/find.js"
  ],
  "bin": {
    "eslint-find-removed-rules": "lib/cli.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:ci": "npm test -- --ci",
    "format": "prettier --ignore-unknown .",
    "format:write": "npm run format -- --write",
    "format:ci": "npm run format -- --check",
    "lint": "eslint --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "semantic-release": "semantic-release",
    "build": "npm run test:ci && npm run format:ci && npm run lint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": "eslint --report-unused-disable-directives --fix",
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "eslint-find-rules": "^3.6.1",
    "lodash.difference": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "eslint": "^7.13.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "semantic-release": "^17.2.3"
  },
  "peerDependencies": {
    "eslint": "^4 || ^5 || ^6 || ^7"
  }
}
